import java.util.*;
public class PrintNewCustID{
	public static void main(String args[]){
		//Fill the code
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int i=1;
        int[] arr = new int[n];
        for(int i=0;i<n;i++){
            arr[i] = sc.nextInt();
        }
        
        System.out.println("Next customer id is "+i);
	}
}
